{
  "name": "Biz1Book-POS-BizDom",
  "version": "3.3.0",
  "private": true,
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "hmr": "ng serve --configuration=hmr --open",
    "build": "ng build --prod --build-optimizer",
    "build-demo": "ng build --prod --build-optimizer --configuration=demo",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"src/**/*.{ts,scss,less}\"",
    "tslint": "tslint \"src/**/*.{ts}\"",
    "stylelint": "stylelint \"src/**/*.{scss,less}\"",
    "postinstall": "ngcc",
    "electron-r": "ng build --base-href ./",
    "electron-l": "ng build --base-href ./ && electron .",
    "electron": "electron .",
    "electron-build": "electron-packager . --win32",
    "electron-build-all": "electron-packager ./ --platform=all --arch=ia32",
    "electron-build-win": "electron-packager ./ --platform=win32 --arch=ia32 --overwrite",
    "electron-help": "electron-packager --help",
    "electron-packer": "ng build --base-href ./ && tsc --p electron && electron electron/dist/main.js",
    "build:package": "electron-packager . MyAwesomeApp --platform=win32 --arch=x64 --out=release/package",
    "build:winstaller": "node ./build.js",
    "assemble": "npm run electron-r && npm run electron-build-win && npm run genexe",
    "asarpck": "cd biz1book-win32-ia32/resources && asar p app app.asar && rmdir /s /q app && npm run genexe",
    "genexe": "node D:/Angular/EXE/gen-installer/genexe.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,scss,less}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{ts}": [
      "tslint"
    ],
    "src/**/*.{scss,less}": [
      "stylelint"
    ]
  },
  "dependencies": {
